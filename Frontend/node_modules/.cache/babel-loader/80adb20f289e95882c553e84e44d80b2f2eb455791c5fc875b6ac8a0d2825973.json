{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      NameImage: null,\n      listSlide: null,\n      slide: null,\n      startIndex: 0,\n      endIndex: 12,\n      step: 12\n    };\n  },\n  methods: {\n    GoProductPage() {\n      this.$router.push({\n        path: '/product'\n      });\n    },\n    async GetData() {\n      const images = await axios.get(\"http://localhost:3000/api/getimage\");\n      console.log(\">>>check image name: \", images.data.DT);\n      this.NameImage = images.data.DT;\n      const list = [];\n      if (this.NameImage.length >= 12) {\n        for (var i = 0; i < this.NameImage.length / 12; i++) {\n          list[i] = i + 1;\n        }\n      } else {\n        this.list = 1;\n      }\n      this.listSlide = list;\n      console.log(\">>>Show list slide: \", this.listSlide);\n    },\n    ChangeSlide(list) {\n      this.slide = list;\n    }\n  },\n  mounted() {\n    this.GetData();\n  },\n  computed: {\n    limitedItems() {\n      if (this.NameImage !== null) {\n        return this.NameImage.slice(this.startIndex + 12, this.endIndex + 12);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","NameImage","listSlide","slide","startIndex","endIndex","step","methods","GoProductPage","$router","push","path","GetData","images","get","console","log","DT","list","length","i","ChangeSlide","mounted","computed","limitedItems","slice"],"sources":["D:\\github\\Luu Code\\Do_an_UD\\Frontend\\src\\components\\ContentOfHome.vue"],"sourcesContent":["<template>\r\n    <!-- nội dung của trang home-->\r\n    <!-- introduce -->\r\n    <div class=\"Content-introduce-Home\">\r\n        <div id=\"carouselExampleControls\" class=\"carousel carousel-dark slide\" data-bs-ride=\"carousel\">\r\n          <div class=\"carousel-inner\">\r\n            <div class=\"carousel-item active\">\r\n              <img src=\"../assets/introduce-fashion.jpeg\" class=\"d-block w-100\" alt=\"...\">\r\n            </div>\r\n            <div class=\"carousel-item\">\r\n              <img src=\"../assets/introduce-fashion1.jpeg\" class=\"d-block w-100\" alt=\"...\">\r\n            </div>\r\n            <div class=\"carousel-item\">\r\n              <img src=\"../assets/introduce-fashion2.jpeg\" class=\"d-block w-100\" alt=\"...\">\r\n            </div>            \r\n          </div>\r\n          <button class=\"carousel-control-prev\" type=\"button\" data-bs-target=\"#carouselExampleControls\" data-bs-slide=\"prev\">\r\n            <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\r\n            <span class=\"visually-hidden\">Previous</span>\r\n          </button>\r\n          <button class=\"carousel-control-next\" type=\"button\" data-bs-target=\"#carouselExampleControls\" data-bs-slide=\"next\">\r\n            <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\r\n            <span class=\"visually-hidden\">Next</span>\r\n          </button>\r\n        </div>\r\n    </div>\r\n    <!--product-->\r\n    <div class=\"product\" >\r\n        <div class=\"intro-product col-11\">\r\n            <div class=\"product-item col-4\">\r\n              <router-link to=\"/top\">\r\n                <img src=\"../assets/go-top.jpeg\" alt=\"\">\r\n              </router-link>             \r\n            </div>\r\n            <div class=\"product-item col-4\">\r\n              <router-link to=\"/accessories\">\r\n                <img src=\"../assets/go-accessories.jpeg\" alt=\"\">\r\n              </router-link>\r\n            </div>\r\n            <div class=\"product-item col-4\">\r\n              <router-link to=\"/bottom\">\r\n                <img src=\"../assets/go-bottom.jpeg\" alt=\"\">\r\n              </router-link>\r\n            </div>\r\n        </div>\r\n        <!-- sale product-->\r\n        <div class=\"sale-product col-10\">\r\n          <h1>NEW ARRIVALS</h1>\r\n          <div id=\"item-sale\" class=\"carousel carousel-dark slide \" data-bs-ride=\"carousel\">\r\n            <div class=\"carousel-indicators\">\r\n              <!-- 3 gach dấu hiệu quyển slide-->\r\n              <button type=\"button\" data-bs-target=\"#item-sale\" data-bs-slide-to=\"0\" class=\"active\" aria-current=\"true\" aria-label=\"Slide 1\"></button>\r\n              <button type=\"button\" data-bs-target=\"#item-sale\" data-bs-slide-to=\"1\" aria-label=\"Slide 2\"></button>\r\n              <button type=\"button\" data-bs-target=\"#item-sale\" data-bs-slide-to=\"2\" aria-label=\"Slide 3\"></button>\r\n            </div>\r\n            <!-- sale product -->\r\n            <div class=\"carousel-inner\" >\r\n              <!-- slide 1 -->\r\n              <div class=\"carousel-item active\" v-for=\"list of listSlide\" :key=\"list\">\r\n                  <div class=\"row flex-wrap\" >\r\n                    <div class=\"col-4 col-md-3\" @click=\"GoProductPage()\" v-for=\"(item, index) of limitedItems\" :key=\"index\">\r\n                        <img :src=\"item.nameImage\" class=\" w-100\" alt=\"\">\r\n                        <h1>{{ item.productName }}</h1>\r\n                        <span>{{ item.price }}đ</span>           \r\n                    </div>\r\n                  </div>\r\n              </div>\r\n\r\n            </div>\r\n            <!-- bottom chuyển silde-->\r\n            <button class=\"carousel-control-prev\" type=\"button\" data-bs-target=\"#item-sale\" data-bs-slide=\"prev\">\r\n              <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\r\n              <span class=\"visually-hidden\">Previous</span>\r\n            </button>\r\n            <button class=\"carousel-control-next\" type=\"button\" data-bs-target=\"#item-sale\" data-bs-slide=\"next\">\r\n              <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\r\n              <span class=\"visually-hidden\">Next</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n  export default {\r\n    name:'Home',\r\n    data() {\r\n      return {\r\n        NameImage: null,\r\n        listSlide: null,\r\n        slide:null,\r\n        startIndex:0,\r\n        endIndex: 12,\r\n        step: 12\r\n      }\r\n    },\r\n    methods:{\r\n      GoProductPage(){\r\n        this.$router.push({path:'/product'})\r\n      },\r\n      async GetData() {\r\n        const images = await axios.get(\"http://localhost:3000/api/getimage\");\r\n        console.log(\">>>check image name: \", images.data.DT);\r\n        this.NameImage =  images.data.DT;\r\n        const list = []\r\n        if(this.NameImage.length >= 12)\r\n        {\r\n          for(var i=0 ;i < (this.NameImage.length /12);i++)\r\n          {\r\n              list[i] = i +1;\r\n          }\r\n        }\r\n        else\r\n        {\r\n          this.list = 1;\r\n        }\r\n        this.listSlide = list;\r\n        console.log(\">>>Show list slide: \",this.listSlide )\r\n      },\r\n      ChangeSlide(list){\r\n        this.slide =list\r\n      }\r\n    },\r\n    mounted() {\r\n      this.GetData();\r\n    },\r\n    computed:{\r\n      limitedItems(){\r\n        if(this.NameImage !== null)\r\n        {\r\n          return this.NameImage.slice(this.startIndex +12, this.endIndex +12);\r\n        }\r\n      }\r\n    }\r\n    \r\n  }\r\n\r\n</script>\r\n\r\n<style scoped>\r\n/* css phần introduce */\r\n    .Content-introduce-Home{\r\n      width: 100%;\r\n      padding: 0 0px;\r\n    }\r\n\r\n    .Content-introduce-Home #carouselExampleControls\r\n    {\r\n        width: 100%;\r\n    }\r\n\r\n  /* css phần product */\r\n    .product{\r\n      width: 100%;\r\n      padding: 20px 0;\r\n      display: flex;\r\n      justify-content: center;\r\n      flex-direction: column;\r\n      align-items: center;\r\n    }\r\n    .product .intro-product\r\n    {\r\n      display: flex;\r\n      \r\n    }\r\n    .product .intro-product a img{\r\n      width: 95%;\r\n    }\r\n    .product .intro-product:hover a:not(:hover)\r\n    {\r\n      opacity: 0.5;\r\n    }\r\n    /* css phần sale product trong phần product */\r\n        .sale-product h1\r\n        {\r\n          margin-top: 30px;\r\n          font-weight: 700;\r\n        }\r\n      /* css phần nút chuyển slide */\r\n        .sale-product .carousel-control-prev\r\n        {\r\n          transform: translateX(-55%);\r\n        }\r\n        .sale-product .carousel-control-next{\r\n          transform: translateX(55%);\r\n        }\r\n    /* css phần slide sản phẩm*/\r\n    .carousel-item .row\r\n    {\r\n      min-height: 780px;\r\n      cursor: pointer;\r\n    }\r\n    .carousel-item .row h1\r\n    {\r\n      font-size: 19px;\r\n      margin: 5px 0;\r\n      font-weight: 400;\r\n      text-align: center;\r\n    }\r\n    .carousel-item .row span\r\n    {\r\n      color: red;\r\n      font-size: 18px;\r\n      font-weight: 600;\r\n      display: flex;\r\n      justify-content: center;\r\n    }\r\n\r\n    /* dấu chuyển slide */\r\n    .carousel-indicators \r\n    {\r\n      transform: translateY(40px);\r\n    }\r\n</style>"],"mappings":";AAoFA,OAAOA,KAAI,MAAO,OAAO;AACvB,eAAe;EACbC,IAAI,EAAC,MAAM;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,IAAI;MACfC,KAAK,EAAC,IAAI;MACVC,UAAU,EAAC,CAAC;MACZC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,aAAaA,CAAA,EAAE;MACb,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAACC,IAAI,EAAC;MAAU,CAAC;IACrC,CAAC;IACD,MAAMC,OAAOA,CAAA,EAAG;MACd,MAAMC,MAAK,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,oCAAoC,CAAC;MACpEC,OAAO,CAACC,GAAG,CAAC,uBAAuB,EAAEH,MAAM,CAACb,IAAI,CAACiB,EAAE,CAAC;MACpD,IAAI,CAAChB,SAAQ,GAAKY,MAAM,CAACb,IAAI,CAACiB,EAAE;MAChC,MAAMC,IAAG,GAAI,EAAC;MACd,IAAG,IAAI,CAACjB,SAAS,CAACkB,MAAK,IAAK,EAAE,EAC9B;QACE,KAAI,IAAIC,CAAC,GAAC,GAAGA,CAAA,GAAK,IAAI,CAACnB,SAAS,CAACkB,MAAK,GAAG,EAAG,EAACC,CAAC,EAAE,EAChD;UACIF,IAAI,CAACE,CAAC,IAAIA,CAAA,GAAG