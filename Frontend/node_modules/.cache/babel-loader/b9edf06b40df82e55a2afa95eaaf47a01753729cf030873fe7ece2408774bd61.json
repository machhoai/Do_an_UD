/div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            product:null,\r\n            priceSale: null,\r\n            startIndex:0,\r\n            endIndex: 10,\r\n        }\r\n    },\r\n    methods:{\r\n        GoProductPage(item){\r\n        this.$router.push({name:'ProductInformation', params:{maProduct: item.maProduct}})\r\n      },\r\n        async GetData(){\r\n            const Type = \"ao\";\r\n            const Product = await axios.post(\"http://localhost:3000/api/productfilter\",\r\n                {Type}\r\n            );           \r\n            // this.product = Product.data.DT\r\n            console.log(\">>>show product: \",Product.data.DT )\r\n            const list =[]\r\n            for(var i =0; i < Product.data.DT.length; i ++)\r\n            {\r\n                const price = parseInt(Product.data.DT[i].price.replace(/\\./g, \"\"));\r\n                list[i] = price - (price*(Product.data.DT[i].sale)/100)\r\n            }\r\n\r\n            //tao cac trang nho\r\n            const listProduct =[]\r\n            const listSale = []\r\n            for(var i=0 ;i < (Product.data.DT.length /10);i++)\r\n            {\r\n                listProduct[i] = Product.data.DT.slice(this.startIndex + 10*(i) , this.endIndex + 10*(i));\r\n                listSale[i] = list.slice(this.startIndex + 10*(i) , this.endIndex + 10*(i));\r\n            }\r\n            this.product = listProduct\r\n            this.priceSale =listSale\r\n            console.log(\">>>show price sale: \",this.priceSale )\r\n        },\r\n        \r\n    },\r\n    mounted(){\r\n        this.GetData()\r\n    },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.container\r\n{\r\n    height: auto;\r\n    position: relative;\r\n    top :40px;\r\n    display: block;\r\n    width: 100%;\r\n    padding-bottom: 50px;\r\n    border-top: 1px solid black;\r\n}\r\n