{"ast":null,"code":"import NotProduct from '@/components/NotProduct.vue';\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      Account: [],\n      purchases: null\n    };\n  },\n  components: {\n    NotProduct\n  },\n  methods: {\n    async GetData() {\n      const mauser = this.Account.mauser;\n      const ShowData = await axios.post(\"http://localhost:3000/api/showbag\", {\n        mauser\n      });\n      if (ShowData.data.DT) {\n        this.purchases = ShowData.data.DT[0].Products;\n        console.log(\">>> ShowData:\", this.purchases);\n        // console.log(\">>> purchase.Products.nameImage:\", this.purchase.nameImage); \n      }\n    },\n\n    async\n  },\n  mounted() {\n    const session = sessionStorage.getItem(\"account\");\n    if (session) {\n      this.Account = JSON.parse(session);\n      console.log(\">>> account:\", this.Account.mauser);\n    }\n    if (this.Account.mauser !== 0) {\n      this.GetData();\n    }\n  },\n  computed: {\n    TongTien() {\n      if (this.purchases !== null) {\n        let pill = 0;\n        for (let i = 0; i < this.purchases.length; i++) {\n          pill += parseInt(this.purchases[i].Purchases.thanhTien.replace(/\\./g, \"\"));\n        }\n        return pill.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, '.');\n      }\n    },\n    CheckShowBag() {\n      if (this.purchases === null) {\n        return true;\n      }\n      return false;\n    }\n  }\n};","map":{"version":3,"names":["NotProduct","axios","data","Account","purchases","components","methods","GetData","mauser","ShowData","post","DT","Products","console","log","async","mounted","session","sessionStorage","getItem","JSON","parse","computed","TongTien","pill","i","length","parseInt","Purchases","thanhTien","replace","toString","CheckShowBag"],"sources":["D:\\github\\Luu Code\\Do_an_UD\\Frontend\\src\\views\\Bag.vue"],"sourcesContent":["<template>\r\n    <h1 class=\"title\">Giỏ Hàng Của Bạn</h1>\r\n    <div v-if=\"CheckShowBag\">\r\n        <NotProduct/>\r\n    </div>\r\n    \r\n    <table class=\"productBag col-10\" v-else>\r\n        <thead>\r\n            <tr>\r\n                <th class=\"product col-6\" scope=\"col\"></th>\r\n                <th class=\"col-3\" scope=\"col\">Số Lượng</th>\r\n                <th class=\"col-3\" scope=\"col\">Số Tiền</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <tr class=\"bagProd\" v-for=\"(purchase,index) of purchases\" :key=\"index\">\r\n                <th class=\"info\" scope=\"row\">\r\n                    <img :src=\"purchase.nameImage\" class=\"image col-5\" alt=\"\">\r\n                    <div class=\"infoproduct col-7\">\r\n                        <p>Mã SP: {{purchase.maProduct}}-{{purchase.Sizes[0].size}}</p>\r\n                        <h1>{{purchase.productName}}</h1>\r\n                        <button type=\"submit\">Remove</button>\r\n                    </div>\r\n                </th>\r\n                <td>{{purchase.Purchases.soLuongMua}}</td>\r\n                <td>{{purchase.Purchases.thanhTien}}</td>\r\n            </tr>\r\n            <tr >\r\n                <th></th>\r\n                <td>Tổng Tiền:</td>\r\n                <td>{{TongTien}}</td>\r\n            </tr>\r\n            <tr>\r\n                <th></th>\r\n                <td></td>\r\n                <td><button type=\"submit\">Thanh Toán</button></td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n</template>\r\n\r\n<script>\r\nimport NotProduct from '@/components/NotProduct.vue'\r\nimport axios from \"axios\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            Account: [],\r\n            purchases: null,\r\n        }\r\n    },\r\n    components:{\r\n        NotProduct\r\n    },\r\n    methods:{\r\n        async GetData(){\r\n            const mauser = this.Account.mauser;\r\n            const ShowData = await axios.post(\"http://localhost:3000/api/showbag\",{mauser})\r\n            if(ShowData.data.DT)\r\n            {\r\n               this.purchases = ShowData.data.DT[0].Products\r\n                console.log(\">>> ShowData:\", this.purchases);\r\n                // console.log(\">>> purchase.Products.nameImage:\", this.purchase.nameImage); \r\n            }\r\n            \r\n        },\r\n        async\r\n        \r\n    },\r\n    mounted(){\r\n        const session = sessionStorage.getItem(\"account\");\r\n        if (session) {\r\n        this.Account = JSON.parse(session);\r\n        console.log(\">>> account:\", this.Account.mauser);\r\n        }\r\n        if(this.Account.mauser !==0){\r\n            this.GetData()\r\n        }\r\n    },\r\n    computed:{\r\n        TongTien(){\r\n            if(this.purchases !== null)\r\n            {\r\n                let pill = 0\r\n                for (let i = 0; i < this.purchases.length; i++) {\r\n                    \r\n                    pill += parseInt((this.purchases[i].Purchases.thanhTien).replace(/\\./g, \"\"))\r\n                }\r\n                return pill.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, '.')\r\n            }\r\n        },\r\n        CheckShowBag(){\r\n            if(this.purchases === null){\r\n                return true\r\n            }\r\n            return false\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* title */\r\n.title{\r\n    margin-top: 30px;\r\n    font-size: 28px;\r\n    font-weight: 600;\r\n\r\n}\r\n\r\n/* css phan table */\r\n    /* row chua anh va thong tin */\r\n    .productBag{\r\n        position: relative;\r\n        left: 50%;\r\n        transform: translateX(-50%);\r\n    }\r\n    .productBag .bagProd{\r\n        \r\n        border-top: 1px solid gray;\r\n        border-bottom: 1px solid gray;\r\n    }\r\n    .productBag .info{\r\n        display: flex;\r\n        margin: 5px 0;\r\n    }\r\n    .productBag .info .image{\r\n        height: 100px;\r\n        width: 100px;\r\n    }\r\n    .productBag .info .infoproduct p{\r\n        font-s