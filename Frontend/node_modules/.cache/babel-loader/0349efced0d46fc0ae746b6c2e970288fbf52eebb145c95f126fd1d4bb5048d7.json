-Forgot p {\r\n  position: absolute;\r\n  right: 0px;\r\n  cursor: pointer;\r\n}\r\n\r\n.Background #Container form #Remember-Forgot input {\r\n  margin-top: 10px;\r\n  height: 18px;\r\n  width: 18px;\r\n  cursor: pointer;\r\n}\r\n.Background #Container form label .User {\r\n  position: absolute;\r\n  top: 10%;\r\n  left: 40px;\r\n  margin: 0px;\r\n  font-size: 17px;\r\n  transition: 0.2s;\r\n}\r\n/* thành phần active của user */\r\n.User-active {\r\n  transform: translateY(-25px);\r\n  font-weight: 600;\r\n  font-size: 18px;\r\n}\r\n.Background #Container form label .Pass {\r\n  position: absolute;\r\n  top: 10%;\r\n  left: 40px;\r\n  margin: 0px;\r\n  font-size: 17px;\r\n  transition: 0.2s;\r\n}\r\n.Pass-active {\r\n  transform: translateY(-25px);\r\n  font-weight: 600;\r\n  font-size: 18px;\r\n}\r\n</style>"],"mappings":";AA8DA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,UAAU;MACrBC,QAAQ,EAAE,IAAI;MACdC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,KAAK;MACnBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACR,SAAQ,GAAI,IAAI,CAACA,SAAQ,KAAM,UAAS,GAAI,MAAK,GAAI,UAAU;MACpE,IAAI,CAACC,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACC,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC,CAAC;IACD,MAAMO,SAASA,CAAA,EAAG;MAChBC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAE,IAAI,CAACR,KAAK,EAAE,GAAG,EAAE,IAAI,CAACC,QAAQ,CAAC;MAC7D,MAAMA,QAAO,GAAI,IAAI,CAACA,QAAQ;MAC9B,MAAMD,KAAI,GAAI,IAAI,CAACA,KAAK;MACxB,MAAMS,YAAW,GAAI,MAAMd,KAAK,CAACe,IAAI,CACnC,oCAAoC,EACpC;QACEV,KAAK;QACLC;MACF,CACF,CAAC;MACDM,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAEC,YAAY,CAACb,IAAI,CAAC;MAC3D,MAAMA,IAAG,GAAI;QACHe,eAAe,EAAE,IAAI;QACrBC,KAAK,EAAE,YAAY;QACnBC,OAAO,EAAE;MACX;MAEF,IAAIJ,YAAY,CAACb,IAAI,CAACkB,EAAC,KAAM,CAAC,EAAE;QAE9BC,cAAc,CAACC,OAAO,CAAC,SAAS,EAAEC,IAAI,CAACC,SAAS,CAACtB,IAAI,CAAC,CAAC;QACvD,IAAI,CAACuB,OAAO,CAACC,IAAI,CAAC;UAACC,IAAI,EAAC;QAAG,CAAC;QAC5BC,UAAU,CAAC,MAAM;UACjBC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC,CAAC;QAC1B,CAAC,